      {
        "type": "image",
        "name": "Product Image",
        "x": 0, "y": 0, "width": 1080, "height": 1080,
        "src": "{ai_generated_image}"
      },
      {
        "type": "text",
        "name": "Title",
        "x": 100, "y": 100,
        "content": "NEW ARRIVAL",
        "font": "Inter",
        "fontSize": 48,
        "fill": "#000000"
      }
    ]
  },
  "tags": ["product", "minimalist", "instagram", "square"]
